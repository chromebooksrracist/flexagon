{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "tests",
      "type": "node",
      "request": "launch",
      "program": "${workspaceRoot}/node_modules/jasmine/bin/jasmine.js",
      "cwd": "${workspaceRoot}",
      "outFiles": [
        "${workspaceRoot}/build/**/*.js"
      ],
      "args": [
        "build/test/src/**/*.js"
      ],
      "sourceMaps": true
    }
  ]
}
